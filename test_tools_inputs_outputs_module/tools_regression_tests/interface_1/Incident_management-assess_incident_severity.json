{
  "env": "incident_management_redos",
  "interface_num": 1,
  "task": {
    "actions": [
      {
        "name": "assess_incident_severity",
        "arguments": {
          "complete_outage": true
        },
        "expected": {
          "severity": "P4"
        }
      },
      {
        "name": "assess_incident_severity",
        "arguments": {
